{
  "expo": {
    "sdkVersion": "33.0.0",
    "name": "Kingdom Death: Monster Companion",
    "slug": "kdm-companion",
    "icon": "./images/icon_lantern.png",
    "version": "5.2.0",
    "githubUrl": "https://github.com/ingoclaro/kdm-companion",
    "orientation": "portrait",
    "assetBundlePatterns": ["images/*"],
    "primaryColor": "#212121",
    "androidStatusBar": {
      "backgroundColor": "#212121"
    },
    "platforms": ["android"],
    "android": {
      "package": "com.github.ingoclaro.kdmcompanion",
      "versionCode": 2019072201,
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.github.ingoclaro.kdmcompanion",
      "permissions": ["WAKE_LOCK"]
    },
    "ios": {
      "bundleIdentifier": "com.github.ingoclaro.kdmcompanion",
      "buildNumber": "2019072201"
    },
    "splash": {
      "image": "./images/logo.png",
      "backgroundColor": "#000000",
      "resizeMode": "contain"
    },
    "privacy": "unlisted",
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "ingo-claro",
            "project": "kingdom-death-monster-companion"
          }
        }
      ]
    }
  }
}
